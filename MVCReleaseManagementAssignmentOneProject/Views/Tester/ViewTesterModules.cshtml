@model IEnumerable<MVCReleaseManagementAssignmentOneProject.Models.ModuleDetails>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewTesterModules</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ModuleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TesterId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModuleStatus)
            </th>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ModuleId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TesterId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ModuleStatus)
                </td>
            </tr>
        }

    </table>
    <br />
    <div>
        @Html.ActionLink("Go Back to Menu", "TesterNavigate")
    </div>
</body>
</html>
